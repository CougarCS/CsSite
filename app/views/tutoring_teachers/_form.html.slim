= form_for @tutoring_teacher do |f|
  - if @tutoring_teacher.errors.any?
    #error_explanation
      h2 = "#{pluralize(@tutoring_teacher.errors.count, "error")} prohibited this tutoring_teacher from being saved:"
      ul
        - @tutoring_teacher.errors.full_messages.each do |message|
          li = message

  .field
    =f.label :member_name
    =f.text_field :member_name
  .field
    = f.label :date
    = f.date_select :date

  .field
    = f.label :note
    = f.text_area :note

  .actions = f.submit
